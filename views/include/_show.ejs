<% for(var i = 0; i < users.length; i++) { %>

<tr
  data-date="<%= users[i].datepicker %>"
  data-jobType="<%= users[i].jobType %>"
>
  <td><%= i + 1 %></td>
  <td><a class="link" href="/employee?id=<%= users[i]._id %>"><%= users[i].name %></a></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].datepicker %></td>
  <td><%= users[i].jobType %></td>
  <td>
    <a
      href="/update_user?id=<%= users[i]._id %>"
      class="btn-border-shadow update"
      ><span class="text-gradient"><i class="fas fa-pencil-alt"></i></span
    ></a>
    <a data-id="<%=users[i]._id %>" class="btn-border-shadow delete"
      ><span class="text-gradient"><i class="fas fa-times"></i></span
    ></a>
  </td>
</tr>

<% } %>
