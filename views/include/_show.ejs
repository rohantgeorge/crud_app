<% for(var i = 0; i < users.length; i++) { %>

<tr>
  <td><%= i + 1 %></td>
  <td><%= users[i].name %></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].gender %></td>
  <td><%= users[i].clockInTime %></td>
  <td><%= users[i].clockOutTime %></td>
  <td><%= users[i].datepicker %></td>
  <td>
    <a
      href="/update_user?id=<%= users[i]._id %>"
      class="btn-border-shadow update"
      ><span class="text-gradient"><i class="fas fa-pencil-alt"></i></span
    ></a>
    <a data-id="<%=users[i]._id %>" class="btn-border-shadow delete"
      ><span class="text-gradient"><i class="fas fa-times"></i></span
    ></a>
  </td>
</tr>

<% } %>
