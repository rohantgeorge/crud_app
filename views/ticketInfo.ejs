<% console.log(allUser[0], "Ticket Info"); %>
<!-- Include Header -->
<%- include('include/_headerAdmin') %>
<!-- /Include Header -->

<!-- Main Site -->
<main class="container main pt-5" id="employeeDashboard">
  <div id="employee_legend" class="row">
    <div class="col col-lg-12 position-relative">
      <a href="/allTickets"
        ><i class="fas fa-angle-double-left"></i> All Tickets
      </a>
      <span hidden class="blue m-0"><%= allUser[0]?._id %></span>
      <h1 class="blue m-0 mt- 5">
        This is a <%= allUser[0]?.ticketType %> request from 
        <%= allUser[0]?.ticketerName %> to <%= allUser[0]?.ticketeeName %>
      </h1>
      <h3>
        <span>Transfer Date: </span>
        <span class="font-weight-light dateFormat"><%= allUser[0]?.transferDate %></span>
        <span id="dayName" class="font-weight-light day"></span>
      </h3>
      <h3>
        <span>Transfer Time: </span>
        <span class="font-weight-light"
          ><%= allUser[0]?.transferClockInTime %> to 
          <%= allUser[0]?.transferClockOutTime %>
        </span>
      </h3>

      <!-- Form Handling -->
      <%- include('include/_ticketUpdateForm') %>
      <!-- ./Form Handling -->
      
    </div>
  </div>
</main>

<!-- /Main Site -->

<!-- Include Footer -->
<%- include('include/_footer') %>
<!-- /Include Footer -->
